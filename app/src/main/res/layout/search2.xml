<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/map_container"
        android:layout_width="match_parent"
        android:layout_height="280dp">
        <fragment
            android:id="@+id/map_fragment"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </FrameLayout>
        <ImageView
            android:background="@color/white_A700"
            android:id="@+id/back"
            android:layout_height="25.35dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="35dp"
            android:layout_width="25.35dp"
            android:paddingHorizontal="2.11dp"
            android:paddingVertical="1.84dp"
            android:src="@drawable/back"/>

        <EditText
            android:id="@+id/mt3"
            android:background="@drawable/search1"
            android:layout_height="50dp"
            android:layout_weight="0.78"
            android:layout_marginStart="60dp"
            android:layout_marginTop="20dp"
            android:layout_width="300dp"/>
    <TextView
        android:id="@+id/mtt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:layout_marginStart="80dp"
        android:fontFamily="@font/interbold"
        android:singleLine="true"
        android:text="라스베이거스"
        android:textColor="@color/gray_800"
        android:textSize="16dp" />


    <TextView
        android:id="@+id/search1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mt3"
        android:layout_marginStart="50dp"
        android:layout_marginTop="230dp"
        android:fontFamily="@font/interbold"
        android:text="미국 라스베이거스"
        android:textColor="@color/gray_800"
        android:textSize="25dp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/search2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/search1"
        android:layout_marginTop="5dp"
        android:layout_marginStart="50dp"
        android:fontFamily="@font/intermedium"
        android:singleLine="true"
        android:text="카지노와 많은 놀거리가 풍부한"
        android:textColor="@color/gray_800"
        android:textSize="14dp" />
    <TextView
        android:id="@+id/search3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/search2"
        android:layout_marginTop="-3dp"
        android:layout_marginStart="50dp"
        android:fontFamily="@font/intermedium"
        android:singleLine="true"
        android:text="사막 위에 지어진 기적 같은 도시"
        android:textColor="@color/gray_800"
        android:textSize="14dp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/image1"/>
    <View
        android:id="@+id/line"
        android:layout_below="@+id/search3"
        android:background="@color/gray_400"
        android:layout_height="1dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:layout_width="370dp"/>
    <ImageView
        android:id="@+id/search4"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_below="@+id/line"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/lvg1" />
    <ImageView
        android:id="@+id/search5"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_below="@+id/line"
        android:layout_alignParentStart="true"
        android:layout_marginStart="145dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/lvg2"/>
    <ImageView
        android:id="@+id/search6"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_below="@+id/line"
        android:layout_alignParentStart="true"
        android:layout_marginStart="270dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/lvg3" />

    <!-- 직항부터 !!! -->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/search6"
        android:layout_centerHorizontal="true">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="0dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="41dp"
                    android:layout_height="35dp"
                    android:src="@drawable/no1"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="직항"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="11시간 소요"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp">

                <ImageView
                    android:layout_width="41dp"
                    android:layout_height="35dp"
                    android:src="@drawable/no2" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="전압"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="120v"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp">

                <ImageView
                    android:layout_width="41dp"
                    android:layout_height="35dp"
                    android:src="@drawable/no3" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 1USD"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1,277.50원"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
    </LinearLayout>




    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="70dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="41dp"
                android:layout_height="35dp"
                android:src="@drawable/no4" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="영어"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="41dp"
                android:layout_height="35dp"
                android:src="@drawable/no5"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="최저 24.6°"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="최고 41.1°"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:layout_width="41dp"
                android:layout_height="35dp"
                android:src="@drawable/no6" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="한국대비"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="16시간 느림"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
            </LinearLayout>
        </LinearLayout>


    </LinearLayout>
    </LinearLayout>



</RelativeLayout>