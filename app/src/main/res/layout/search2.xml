<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/map_container"
        android:layout_width="match_parent"
        android:layout_height="330dp">
        <fragment
            android:id="@+id/map_fragment"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </FrameLayout>
    <LinearLayout android:id="@+id/box1"
        android:layout_height="wrap_content"
        android:layout_marginEnd="29.76dp"
        android:layout_marginStart="29.76dp"
        android:layout_marginTop="14dp"
        android:layout_width="match_parent" android:orientation="horizontal">


        <EditText
            android:id="@+id/mt3"
            android:background="@drawable/rectangle_border_gray_401_radius_7"
            android:layout_height="50dp"
            android:layout_weight="0.78"
            android:layout_width="0dp"/>

    </LinearLayout>
    <TextView
        android:id="@+id/search1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/box1"
        android:layout_marginStart="50dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/interbold"
        android:text="일본 후쿠오카"
        android:textColor="@color/gray_800"
        android:textSize="25dp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/search2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/search1"
        android:layout_marginTop="5dp"
        android:layout_marginStart="50dp"
        android:fontFamily="@font/intermedium"
        android:singleLine="true"
        android:text="돈코츠 라멘의 본고장으로 유명하며"
        android:textColor="@color/gray_800"
        android:textSize="14dp" />
    <TextView
        android:id="@+id/search3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/search2"
        android:layout_marginTop="-3dp"
        android:layout_marginStart="50dp"
        android:fontFamily="@font/intermedium"
        android:singleLine="true"
        android:text="시내에서 공항이 가장 가까운 규슈의 대표적 관광도시"
        android:textColor="@color/gray_800"
        android:textSize="14dp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/image1"/>
    <View
        android:id="@+id/line"
        android:layout_below="@+id/search3"
        android:background="@color/gray_400"
        android:layout_height="1dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:layout_width="370dp"/>
    <ImageView
        android:id="@+id/search4"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_below="@+id/line"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/bali" />
    <ImageView
        android:id="@+id/search5"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_below="@+id/line"
        android:layout_alignParentStart="true"
        android:layout_marginStart="145dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/bali" />
    <ImageView
        android:id="@+id/search6"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_below="@+id/line"
        android:layout_alignParentStart="true"
        android:layout_marginStart="270dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/bali" />

    <!-- 직항부터 !!! -->
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/search6"
        android:layout_centerHorizontal="true">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="0dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="41dp"
                    android:layout_height="35dp"
                    android:src="@drawable/no1"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="직항"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="11시간 소요"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp">

                <ImageView
                    android:layout_width="41dp"
                    android:layout_height="35dp"
                    android:src="@drawable/no2" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="전압"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="120v"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp">

                <ImageView
                    android:layout_width="41dp"
                    android:layout_height="35dp"
                    android:src="@drawable/no3" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" 1USD"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1,277.50원"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_marginLeft="3dp"/>
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
    </LinearLayout>




    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="70dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="41dp"
                android:layout_height="35dp"
                android:src="@drawable/no4" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="영어"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="41dp"
                android:layout_height="35dp"
                android:src="@drawable/no5"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="최저 24.6°"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="최고 41.1°"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:layout_width="41dp"
                android:layout_height="35dp"
                android:src="@drawable/no6" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="한국대비"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="16시간 느림"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginLeft="3dp"/>
            </LinearLayout>
        </LinearLayout>


    </LinearLayout>
    </LinearLayout>



</RelativeLayout>