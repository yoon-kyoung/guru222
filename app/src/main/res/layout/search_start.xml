<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameStackarrowleft"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    style="@style/groupStylewhite_A700">

    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_gravity="top|center"
        android:layout_height="match_parent"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="10.56dp"
        android:layout_marginStart="10.56dp"
        android:layout_marginTop="18.37dp"
        android:layout_width="match_parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <LinearLayout
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <ImageButton
                    android:background="@color/white_A700"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="25.35dp"
                    android:layout_marginBottom="7.35dp"
                    android:layout_marginTop="12dp"
                    android:layout_width="25.35dp"
                    android:paddingHorizontal="2.11dp"
                    android:paddingVertical="1.84dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft"/>

                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                    android:id="@+id/searchViewGroupOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8.45dp"
                    android:layout_weight="0.93"
                    android:layout_width="0dp"
                    android:theme="@style/searchViewGroupOne"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="떠나고 싶은 여행지를 찾아보세요" />
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>


        <LinearLayout
            android:layout_marginStart="10.56dp"
            android:layout_marginTop="30dp"
            android:id="@+id/linearRow"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linearRowclock"
                android:layout_height="wrap_content"
                android:layout_weight="0.62"
                android:layout_width="0dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageClock"
                    android:layout_width="14dp"
                    android:layout_height="14dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="2dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_clock" />

                <TextView
                    android:ellipsize="end"
                    android:id="@+id/txtOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15.85dp"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:text="코타키나발루"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRow0501"
                android:layout_height="wrap_content"
                android:layout_marginStart="153.18dp"
                android:layout_weight="0.38"
                android:layout_width="0dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt0501"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.73"
                    android:layout_width="0dp"
                    android:textStyle="bold"
                    android:textColor="@color/bluegray_100"
                    android:text="07.18" />

                <ImageView android:id="@+id/imageClose"
                    android:layout_height="14.79dp"
                    android:layout_marginBottom="1.84dp"
                    android:layout_marginStart="19.02dp"
                    android:layout_marginTop="2.76dp"
                    android:layout_width="14.79dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close_gray_500"/>
            </LinearLayout>
        </LinearLayout>

        <ImageView android:id="@+id/imageGroup14193"
            android:layout_gravity="center_horizontal"
            android:layout_height="119.37dp"
            android:layout_marginEnd="22dp"
            android:layout_marginStart="22dp"
            android:layout_marginTop="143.28dp"
            android:layout_width="235.58dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_group14193"/>
    </LinearLayout>
</FrameLayout>
